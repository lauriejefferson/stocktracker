<div class="mt-5">
<h1>Stock Tracker App</h1>
<%= turbo_frame_tag "stock" do %>
  <%= render "form" %>
  <div class="mt-3">
    <% if @stocks %>
      <div class="d-flex p-3 shadow-sm bg-body rounded">
        <%= @stocks.stocks[0].name %>
      </div>
      
    <% end %>
  </div>
<% end %>
</div>


